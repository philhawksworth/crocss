{
  "imports": {
    "hono": "jsr:@hono/hono@^4.8.12",
    "lume/": "https://deno.land/x/lume@v3.0.5/",
    "lume/jsx-runtime": "https://deno.land/x/ssx@v0.1.11/jsx-runtime.ts"
  },
  "tasks": {
    "build": "deno run --allow-read --allow-write build.ts",
    "start": "deno run --watch --allow-read --allow-write --allow-net --allow-env main.ts",
    "serve": "deno run --allow-read --allow-net --allow-env main.ts"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "lume"
  },
  "unstable": [
    "temporal",
    "fmt-component"
  ],
  "lint": {
    "plugins": [
      "https://deno.land/x/lume@v3.0.5/lint.ts"
    ]
  }
}
